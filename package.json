{
	"name": "split-pane",
	"displayName": "split pane",
	"description": "a simple extension to open a file on a new split pane",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:splitPane.openInNewSplit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "splitPane.openInNewSplit",
			"title": "SplitPane: open in new split"
		}],
		"keybindings": [
			{
					"command": "splitPane.openInNewSplit",
					"key": "ctrl+f12",
					"mac": "cmd+f12",
					"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
